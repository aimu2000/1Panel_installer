#!/bin/sh /etc/rc.common

### BEGIN INIT INFO
# Provides:          1panel-agent
# Required-Start:    $network $local_fs 1panel-core
# Required-Stop:     $network $local_fs
# Default-Start:     2 3 4 5
# Default-Stop:      0 1 6
# Short-Description: 1Panel Agent Service Daemon
# Description:       1Panel Agent management daemon
### END INIT INFO

NAME="1panel-agent"
DAEMON="/usr/bin/1panel-agent"

USE_PROCD=1
START=96
STOP=14
start_service() {
    procd_open_instance
    procd_set_param command "$DAEMON"
    procd_set_param stdout 0
    procd_set_param stderr 1
    procd_close_instance
}
